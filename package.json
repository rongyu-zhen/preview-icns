{
  "name": "preview-icns",
  "displayName": "preview-icns",
  "description": "preview .icns file.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "gocount.preview-icns",
        "displayName": "Preview ICNS",
        "selector": [
          {
            "filenamePattern": "*.{icns,ICNS}"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "preview-icns.zoomIn",
        "title": "%command.zoomIn%",
        "category": "ICNS Preview"
      },
      {
        "command": "preview-icns.zoomOut",
        "title": "%command.zoomOut%",
        "category": "ICNS Preview"
      },
      {
        "command": "preview-icns.copyImage",
        "title": "%command.copyImage%",
        "category": "ICNS Preview"
      },
      {
        "command": "preview-icns.switchResolution",
        "title": "%command.switchResolution%",
        "category": "ICNS Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/humps": "^2.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "html-element-attributes-typescript": "^1.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "vscode-uri": "^3.0.8"
  }
}
